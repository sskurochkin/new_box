extends ../../layout/layout

block vars
	- var pageTitle = 'Каталог'
	- var pageBodyClass = ''
	- var dir = './'


block main
	.container
		+breadcrumb({
			content2 : 'Каталог',

		})
		+page-header-section({
			title: "Каталог",
			sidemenu: false
			})
